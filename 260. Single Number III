https://leetcode.com/problems/single-number-iii/description/?envType=daily-question&envId=2024-05-31


class Solution {
public:
    vector<int> singleNumber(vector<int>& nums) {
        int xor_ = 0;
        for(auto &el : nums)    xor_ ^= el;
        int mask = 0;
        for(int i=0; i<31; i++){
            if((1 << i) & xor_) {   
                mask = 1<<i ;
                break;
            }
        }
        int n1 = 0, n2 = 0;
        for(auto &el : nums){
            if(el & mask)   n1 ^= el;
            else    n2 ^= el;
        }
        return {n1, n2};

    }
};
